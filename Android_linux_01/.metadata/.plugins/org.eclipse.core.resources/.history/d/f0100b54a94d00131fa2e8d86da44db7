package com.mingzz.server;

import java.io.Serializable;

public class TransmissionData implements Serializable{
	
	
	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	
	private int transmissionFlag ; 
	private UserMessage userMessage ;
	
	public TransmissionData(){
		transmissionFlag = 0;
		userMessage = null;
	}
	public TransmissionData(int transmissionFlag, UserMessage userMessage){
		this.transmissionFlag = transmissionFlag;
		this.userMessage = userMessage;
	}
	public int getTransmissionFlag(){
		return transmissionFlag;
	}
	public void setTransmissionFlag(int transmissionFlag){
		this.transmissionFlag = transmissionFlag;
	}
	public UserMessage getUserMessage(){
		return userMessage;
	}
	public void setUserMessage(UserMessage userMessage){
		this.userMessage = userMessage;
	}
}
